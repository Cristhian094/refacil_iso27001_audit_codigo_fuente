#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/d3-dsv/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/d3-dsv/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/node_modules:/mnt/c/Users/ximen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/d3-dsv/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/d3-dsv/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/node_modules:/mnt/c/Users/ximen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../d3-dsv/bin/json2dsv.js" "$@"
else
  exec node  "$basedir/../d3-dsv/bin/json2dsv.js" "$@"
fi
