    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/autoprefixer@10.4.21_postcss@8.5.6/node_modules/autoprefixer/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/autoprefixer@10.4.21_postcss@8.5.6/node_modules/autoprefixer/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/autoprefixer@10.4.21_postcss@8.5.6/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/autoprefixer@10.4.21_postcss@8.5.6/node_modules/autoprefixer/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/autoprefixer@10.4.21_postcss@8.5.6/node_modules/autoprefixer/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/autoprefixer@10.4.21_postcss@8.5.6/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules:$NODE_PATH"
