#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/typescript@5.6.3/node_modules/typescript/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/typescript@5.6.3/node_modules/typescript/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/typescript@5.6.3/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/typescript@5.6.3/node_modules/typescript/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/typescript@5.6.3/node_modules/typescript/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/typescript@5.6.3/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
