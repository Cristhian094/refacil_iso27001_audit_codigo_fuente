else
  export NODE_PATH="/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/rollup@4.52.4/node_modules/rollup/dist/bin/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/rollup@4.52.4/node_modules/rollup/dist/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/rollup@4.52.4/node_modules/rollup/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/rollup@4.52.4/node_modules:/mnt/c/Users/ximen/OneDrive - Refacil/Onedrive/OneDrive - Refacil/ISO 27001/Capacitaciones/refacil_iso27001_audit_codigo_fuente/node_modules/.pnpm/node_modules:$NODE_PATH"
